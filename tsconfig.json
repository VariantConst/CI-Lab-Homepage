{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "plugins": [
      {
        "name": "@astrojs/ts-plugin",
        "typeRoots": [
          "./node_modules/@types",
          "./types"
        ]
      }
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "./typings.d.ts",
    "./src/**/*"
  ]
}